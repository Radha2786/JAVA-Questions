+package Recursion.MAZE_PROBLEMS;
+
+public class Move_Horizontaly_vertically {
+    public static void main(String[] args) {
+        int board_length = 4;
+        String ans ="";
+        Total_path(board_length,0,0,ans);
+    }
+    static void Total_path(int board_length,int current_row,int current_col, String ans){
+        if(current_row==board_length-1 && current_col==board_length-1){
+            System.out.println(ans);
+            return;
+
+        }
+        if(current_row==board_length || current_col==board_length){
+            return;
+
+        }
+//        moving vertically
+        Total_path(board_length,current_row+1,current_col,ans+'V');
+//        moving horizontally
+        Total_path(board_length,current_row,current_col+1,ans+'H');
+
+    }
+}
