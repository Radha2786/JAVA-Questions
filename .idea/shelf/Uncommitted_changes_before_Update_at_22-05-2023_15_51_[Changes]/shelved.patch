----+        for(int i=1;i<=n;i++){
----+            t=t+1;
----+            if(t==time){
----+//                System.out.println(i+1);
----+                ans=i+1;
----+                flag=true;
----+            }
----+        }
----+        if (flag==true){
----+            System.out.println(ans);;
----+        }
----+        while(t>=n){
----+            for(int j=n-1;j>=1;j--){
----+                t=t+1;
----+                if(t==time){
----+                    System.out.println(j-1);
----+                    break;
----+                }
